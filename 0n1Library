	Tabs_1.Position = UDim2.new(0.0171673819, 0,0.146031752, 0)
	Tabs_1.Size = UDim2.new(0, 156,0, 229)
	Tabs_1.ClipsDescendants = true
	Tabs_1.AutomaticCanvasSize = Enum.AutomaticSize.Y
