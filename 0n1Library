	Tabs_1.Name = "Tabs"
	Tabs_1.Parent = Main_1
	Tabs_1.BackgroundColor3 = Color3.fromRGB(36,36,36)
	Tabs_1.BorderColor3 = Color3.fromRGB(27,42,53)
