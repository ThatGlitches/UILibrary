	local UIListLayout_1 = Instance.new("UIListLayout")
	local Close_1 = Instance.new("TextButton")
	local Items_1 = Instance.new("Frame")

	
	
	ScreenGui.Parent = game.CoreGui

	Main_1.Name = "Main"
	Main_1.Parent = ScreenGui
